<Window x:Class="BFH.Demo.ClientUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BFH.Demo.ClientUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <DockPanel>
            <StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Right">
                <Button Click="AppDomain_Clicked">App Domain</Button>
                <Button Click="SetValue_Clicked">Set Value</Button>
                <Button Click="GetValue_Clicked">Get Value</Button>
                <Button Click="NextEnum_Clicked">Next Enum</Button>
                <Button Click="FillList_Clicked">Fill in List</Button>
            </StackPanel>
            <TextBlock Name="AppDomainNameServer" DockPanel.Dock="Top" />
            <TextBox Name="ValueEntry" DockPanel.Dock="Top" />
            <TextBlock Name="ValueRead" DockPanel.Dock="Top" />
            <ComboBox Name="EnumValue" DockPanel.Dock="Top"/>
            <ListBox Name="DetailList"  />
        </DockPanel>
        
    </Grid>
</Window>
